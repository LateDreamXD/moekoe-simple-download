{
  "manifest_version": 3,
  "moekoe": true,
  "name": "MoeKoe Music Simple Download",
  "version": "%version%",
  "description": "MoeKoe Music 简单下载插件 —— 轻量 · 简洁 · 开源",
  "permissions": ["scripting"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.iife.js"],
    "css": ["content.css"],
    "run_at": "document_end"
  }],
  "icons": {
    "256": "icon.png"
  }
}
